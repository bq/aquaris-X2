 };

&spmi_bus {
	qcom,pm660@0 {
		compatible ="qcom,spmi-pmic";
		qcom,power-on@800 {
			compatible = "qcom,qpnp-power-on";
			qcom,s3-debounce = <16>;           //16 seconds to trigger S3 reset
			qcom,s3-src = "kpdpwr-and-resin";  //S3 trig by KPDPWR_ON+RESIN (resin = VOL-)
			qcom,pon_1 {
				qcom,pon-type = <0>;       //KPDPWR_ON
				qcom,pull-up = <1>;        //Pull-up enabled
				qcom,support-reset = <1>;
				qcom,s1-timer = <10256>;   //10256 ms to bark (S1)
				qcom,s2-timer = <2000>;    //2000 ms from bark to trigger S2 reset
				qcom,s2-type = <8>;        //dVdd Hard Reset
				linux,code = <116>;        //PWR_KEY
			};
			qcom,pon_2 {
				qcom,pon-type = <1>;       //RESIN
				qcom,pull-up = <1>;        //Pull-up enabled
				linux,code = <114>;        //VOL- key
			};
		};
	};

	pm660l_3: qcom,pm660l@3 {
		qcom,leds@d000 {
			red_led: qcom,rgb_0 {
				qcom,start-idx = <0>;
				qcom,idx-len = <6>;
				qcom,duty-pcts =
				[00 00 00 64 64 64];
				qcom,use-blink;
			};

			green_led: qcom,rgb_1 {
				qcom,start-idx = <7>;
				qcom,idx-len = <6>;
				qcom,duty-pcts =
				[00 00 00 64 64 64];
				qcom,use-blink;
			};

			blue_led: qcom,rgb_2 {
				qcom,start-idx = <13>;
				qcom,idx-len = <6>;
				qcom,duty-pcts =
				[00 00 00 64 64 64];
				qcom,use-blink;
			};

		};
